<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.biojavax.bio.seq.SimpleRichFeatureRelationship" table="seqfeature_relationship">

        <id name="id" type="integer" unsaved-value="null" column="seqfeature_relationship_id">
            <generator class="sequence">
                <param name="sequence">seqfeature_relationship_pk_seq</param>
            </generator>
        </id>

        <many-to-one name="term" class="org.biojavax.ontology.SimpleComparableTerm" column="term_id" not-null="true" cascade="persist,merge,save-update"/>
        <many-to-one name="subject" class="org.biojavax.bio.seq.SimpleRichFeature" column="subject_seqfeature_id" not-null="true" cascade="persist,merge,save-update"/>
        <property name="rank"/>
        
    </class>

</hibernate-mapping>