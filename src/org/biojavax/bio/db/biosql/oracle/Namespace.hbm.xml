<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.biojavax.SimpleNamespace" table="biodatabase">

        <id name="id" type="integer" unsaved-value="null" column="biodatabase_id">
            <generator class="sequence">
                <param name="sequence">biodatabase_pk_seq</param>
            </generator>
        </id>

        <property name="name"/>
        <property name="authority"/>
        <property name="description"/>
        <property name="acronym"/>
        <property name="URIString" column="uri"/>

    </class>

</hibernate-mapping>