      </atomicMapping>

      <atomicMapping char="W">
        <symbolref name="TRP" />
      </atomicMapping>

      <atomicMapping char="Y">
        <symbolref name="TYR" />
      </atomicMapping>

      <atomicMapping char="V">
        <symbolref name="VAL" />
      </atomicMapping>

      <ambiguityMapping char="X">
        <symbolref name="ALA"/>
        <symbolref name="ARG"/>
        <symbolref name="ASN"/>
        <symbolref name="ASP"/>
        <symbolref name="CYS"/>
        <symbolref name="GLN"/>
        <symbolref name="GLU"/>
        <symbolref name="GLY"/>
        <symbolref name="HIS"/>
        <symbolref name="ILE"/>
        <symbolref name="LEU"/>
        <symbolref name="LYS"/>
        <symbolref name="MET"/>
        <symbolref name="PHE"/>
