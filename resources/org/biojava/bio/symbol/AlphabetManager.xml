        <symbolref name="SER"/>
        <symbolref name="THR"/>
        <symbolref name="SEC"/>
        <symbolref name="TRP"/>
        <symbolref name="TYR"/>
        <symbolref name="VAL"/>
      </ambiguityMapping>

      <ambiguityMapping char="B">
        <symbolref name="ASP" />
	<symbolref name="ASN" />
      </ambiguityMapping>

      <ambiguityMapping char="Z">
        <symbolref name="GLU" />
	<symbolref name="GLN" />
      </ambiguityMapping>

      <ambiguityMapping char="-">
      </ambiguityMapping>

      <ambiguityMapping char="~">
      </ambiguityMapping>

      <ambiguityMapping char=".">
      </ambiguityMapping>
    </characterTokenization>
  </alphabet>

