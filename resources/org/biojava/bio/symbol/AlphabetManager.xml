  <alphabet name="STRUCTURE">
    <symbol name="nothing">
      <description>The structure here is undetermined</description>
    </symbol>

    <symbol name="coil">
      <description>Random coil</description>
    </symbol>

    <symbol name="alpha-helix">
      <description>part of an alpha-helix</description>
    </symbol>

    <symbol name="3(10)-helix">
    </symbol>

    <symbol name="Pi-helix">
    </symbol>

    <symbol name="sheet">
      <description>part of an extended hydrogen bonded beta-strand (extended strand, logs of Bs)</description>
    </symbol>

    <symbol name="beta-bridge">
      <description>like a lone E</description>
    </symbol>

    <symbol name="turn">
    </symbol>

    <symbol name="bend">
      <description>five-residue bend centred at residue i</description>
    </symbol>

    <characterTokenization name="token" caseSensitive="false">
      <atomicMapping char=" ">
        <symbolref name="nothing" />
      </atomicMapping>

      <atomicMapping char="c">
        <symbolref name="coil" />
      </atomicMapping>

      <atomicMapping char="h">
        <symbolref name="alpha-helix" />
      </atomicMapping>

      <atomicMapping char="g">
        <symbolref name="3(10)-helix" />
      </atomicMapping>

      <atomicMapping char="i">
        <symbolref name="Pi-helix" />
      </atomicMapping>

      <atomicMapping char="e">
        <symbolref name="sheet" />
      </atomicMapping>

      <atomicMapping char="b">
        <symbolref name="beta-bridge" />
      </atomicMapping>

      <atomicMapping char="t">
        <symbolref name="turn" />
      </atomicMapping>

      <atomicMapping char="s">
        <symbolref name="bend" />
      </atomicMapping>

      <ambiguityMapping char="-">
      </ambiguityMapping>
    </characterTokenization>
  </alphabet>

