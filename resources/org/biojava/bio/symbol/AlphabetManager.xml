      <atomicMapping char="G">
        <symbolref name="GLY" />
      </atomicMapping>
      
      <atomicMapping char="H">
        <symbolref name="HIS" />
      </atomicMapping>

      <atomicMapping char="I">
        <symbolref name="ILE" />
      </atomicMapping>

      <atomicMapping char="L">
        <symbolref name="LEU" />
      </atomicMapping>

      <atomicMapping char="K">
        <symbolref name="LYS" />
