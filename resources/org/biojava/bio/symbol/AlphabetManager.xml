<?xml version="1.0"?>

<!--

<!DOCTYPE alphabetManager [
  <!ELEMENT alphabetManager (symbol|alphabet)*>
  <!ELEMENT symbol (short, long?, description?)>
  <!ATTLIST symbol
    name	ID	#IMPLIED>

  <!ELEMENT short (#PCDATA)>
  <!ELEMENT long (#PCDATA)>
  <!ELEMENT description (#PCDATA)>

  <!ELEMENT ambiguity (symbol, symbolref*)>



  <!ELEMENT alphabet (description?, (symbol|symbolref)*, ambiguity*)>

  <!ATTLIST alphabet
    name ID #IMPLIED
    parent IDREF #IMPLIED>



  <!ELEMENT name (#PCDATA)>
  <!ELEMENT symbolref EMPTY>
  <!ATTLIST symbolref
    name	IDREF	#REQUIRED>

]>

-->

<alphabetManager>
  <symbol name="adenine">
    <description>pairs with T in DNA, and U in RNA</description>
  </symbol>

  <symbol name="guanine">
    <description>pairs with C</description>
  </symbol>

  <symbol name="cytosine">
    <description>pairs with G</description>
  </symbol>

  <symbol name="thymine">
    <description>pairs with A</description>
  </symbol>

  <symbol name="uracil">
    <description>pairs with A, replacing T in RNA</description>
  </symbol>



  <alphabet name="DNA">
    <description>Deoxy-Ribose Nucleic Acid. The stuff of life.</description>
    <symbolref name="adenine"/>
    <symbolref name="guanine"/>
    <symbolref name="cytosine"/>
    <symbolref name="thymine"/>
    
    <characterTokenization name="token" caseSensitive="false">
      <atomicMapping char="a">
        <symbolref name="adenine"/>
      </atomicMapping>

      <atomicMapping char="g">
        <symbolref name="guanine"/>
      </atomicMapping>

      <atomicMapping char="c">
        <symbolref name="cytosine"/>
      </atomicMapping>

      <atomicMapping char="t">
        <symbolref name="thymine"/>
      </atomicMapping>

      <ambiguityMapping char="r">
        <symbolref name="adenine" />
	<symbolref name="guanine" />
      </ambiguityMapping>

      <ambiguityMapping char="y">
        <symbolref name="cytosine" />
	<symbolref name="thymine" />
      </ambiguityMapping>

      <ambiguityMapping char="m">
        <symbolref name="cytosine" />
	<symbolref name="adenine" />
      </ambiguityMapping>

      <ambiguityMapping char="k">
        <symbolref name="guanine" />
	<symbolref name="thymine" />
      </ambiguityMapping>

      <ambiguityMapping char="s">
        <symbolref name="guanine" />
	<symbolref name="cytosine" />
      </ambiguityMapping>

      <ambiguityMapping char="w">
        <symbolref name="adenine" />
	<symbolref name="thymine" />
      </ambiguityMapping>

      <ambiguityMapping char="h">
        <symbolref name="adenine" />
	<symbolref name="cytosine" />
	<symbolref name="thymine" />
      </ambiguityMapping>

      <ambiguityMapping char="b">
        <symbolref name="guanine" />
	<symbolref name="cytosine" />
	<symbolref name="thymine" />
      </ambiguityMapping>

      <ambiguityMapping char="v">
        <symbolref name="guanine" />
	<symbolref name="adenine" />
	<symbolref name="cytosine" />
      </ambiguityMapping>

      <ambiguityMapping char="d">
        <symbolref name="guanine" />
	<symbolref name="adenine" />
	<symbolref name="thymine" />
      </ambiguityMapping>
    
      <ambiguityMapping char="n">
        <symbolref name="guanine" />
	<symbolref name="adenine" />
	<symbolref name="cytosine" />
	<symbolref name="thymine" />
      </ambiguityMapping>

      <ambiguityMapping char="x">
        <symbolref name="guanine" />
	<symbolref name="adenine" />
	<symbolref name="cytosine" />
	<symbolref name="thymine" />
      </ambiguityMapping>

      <ambiguityMapping char="-">
        <!-- Gap is the empty set -->
      </ambiguityMapping>

      <ambiguityMapping char="~">
      </ambiguityMapping>

      <ambiguityMapping char=".">
      </ambiguityMapping>
    </characterTokenization>
  </alphabet>



  <alphabet name="RNA">
    <description>Ribose Nucleic Acid. Intermediate between genomic DNA and protein.</description>
    <symbolref name="adenine"/>	
    <symbolref name="guanine"/>
    <symbolref name="cytosine"/>
    <symbolref name="uracil"/>

    <characterTokenization name="token" caseSensitive="false">
      <atomicMapping char="a">
        <symbolref name="adenine"/>
      </atomicMapping>

      <atomicMapping char="g">
        <symbolref name="guanine"/>
      </atomicMapping>

      <atomicMapping char="c">
        <symbolref name="cytosine"/>
      </atomicMapping>

      <atomicMapping char="u">
        <symbolref name="uracil"/>
      </atomicMapping>

      <ambiguityMapping char="-">
        <!-- Gap is the empty set -->
      </ambiguityMapping>

      <ambiguityMapping char="~">
      </ambiguityMapping>

      <ambiguityMapping char=".">
      </ambiguityMapping>
    </characterTokenization>
  </alphabet>


  <alphabet name="PROTEIN">
    <description>Unmodified Amino-acids commonly found in proteins</description>
    <symbol name="ALA">
    </symbol>

    <symbol name="ARG">
    </symbol>

    <symbol name="ASN">
    </symbol>

    <symbol name="ASP">
    </symbol>

    <symbol name="CYS">
    </symbol>

    <symbol name="GLN">
    </symbol>

    <symbol name="GLU">
    </symbol>

    <symbol name="GLY">
    </symbol>

    <symbol name="HIS">
    </symbol>

    <symbol name="ILE">
